{
  "name": "alaa-elsaid/laravel-tap-payments",
  "description": "Tap Payment REST API package for Laravel >= 5",
  "license": "MIT",
  "authors": [
    {
      "name": "Alaa Elsaid",
      "email": "alaa.elboray1@gmail.com",
      "homepage": "https://alaahany.name/"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "illuminate/support": ">=5.6",
    "guzzlehttp/guzzle": ">=6.0"
  },
  "autoload": {
    "psr-4": {
      "Alaa\\TapPayment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alaa\\TapPayment\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alaa\\TapPayment\\TapPaymentServiceProvider"
      ]
    }
  }
}
